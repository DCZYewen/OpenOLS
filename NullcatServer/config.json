{
  "server": {
    "request_timeout": 10,
    "daemon": false,
    "loop_debug": false,
    "handler": {
      "*": [
        "upload.urls",
        "core.urls"
      ]
    }
  },
  "http": {
    "host": "",
    "port": 8086,
    "is_enable": true,
    "rewrite_only": false
  },
  "https": {
    "host": "",
    "port": 443,
    "is_enable": false,
    "support_ciphers": "ECDH+AESGCM:DH+AESGCM:ECDH+AES256:DH+AES256:ECDH+AES128",
    "cert_path": "",
    "key_path": ""
  },
  "database": {
    "database_url": "sqlite:///database.db",
    "use_memcached": false,
    "memcached_url": "",
    "debug": false
  },
  "logger": {
    "level": 20,
    "formatter": "$(asctime)s [$(levelname)s]:$(message)s",
    "time_format": "$Y/$m/$d $H:$M:$S",
    "save_log": true,
    "save_path": "log/"
  },
  "template": {
    "template_path": "template/",
    "use_fs_cache": true,
    "cache_path": "__pycache__/"
  }
}